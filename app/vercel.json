{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",

  "functions": {
    "app/api/**": {
      "maxDuration": 30
    }
  },
  "redirects": [
    {
      "source": "/login",
      "destination": "/login",
      "permanent": false,
      "has": [
        {
          "type": "cookie",
          "key": "session",
          "value": "authenticated"
        }
      ]
    }
  ]
}